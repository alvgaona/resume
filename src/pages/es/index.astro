---
import Layout from "@/layouts/Layout.astro";
import ResumeView from "@/components/resume-view.astro";
import CommandMenu from "@/components/command-menu";
import { data } from "@/i18n/es/resume-data";
import { ui } from "@/i18n/ui";

const labels = ui.es;
---

<Layout meta={{ title: labels["meta.title"] }}>
  <ResumeView data={data} labels={labels} showProjects={true} />
  <CommandMenu
    client:visible
    links={[{ url: "https://alvgaona.com", title: labels["command.website"] }]}
    labels={labels}
  />
</Layout>
